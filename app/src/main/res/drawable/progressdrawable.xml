<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/_5sdp"/>
            <gradient android:startColor="#fdfdfd" android:endColor="#02151a" android:angle="0" android:centerColor="#5e6f76"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/_5sdp"/>
                <gradient android:startColor="#a8cc77" android:endColor="#28ccba" android:angle="0" android:centerColor="#75cc92"/>
            </shape>
        </clip>
    </item>
</layer-list>
